{
  "name": "@neondatabase/toolkit",
  "module": "index.ts",
  "type": "module",
  "version": "0.1.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
