    "test": "bun test --tsconfig ./tsconfig.test.json",
    "prepublishOnly": "bun run build && bun run test"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@neondatabase/api-client": "^2.2.0",
    "@neondatabase/serverless": "^1.0.0"
  }
}
